{
  "name": "api_concessionaria_de_automoveis",
  "version": "1.0.0",
  "description": "API REST para gerenciamento de uma concessionária de automóveis (Express, TypeScript, Prisma, PostgreSQL, Swagger e Zod)",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ClaytonAugust/api_concessionaria_de_automoveis.git"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "swagger",
    "zod",
    "postgresql",
    "api",
    "concessionaria"
  ],
  "author": "Clayton Augusto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ClaytonAugust/api_concessionaria_de_automoveis/issues"
  },
  "homepage": "https://github.com/ClaytonAugust/api_concessionaria_de_automoveis#readme",
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.8.6",
    "eslint": "^9.10.0",
    "nodemon": "^3.1.0",
    "prisma": "^6.16.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}